#include <iostream>
using namespace std;

int main(){
	string a,b;
	
	
	cout<<"ADU VIRUS"<<endl;
	cout<<"========="<<endl;
	cout<<"Masukkan Virus 1 = "<<endl;
	cin>>a;
	
	cout<<"Masukkan Virus 2 = "<<endl;
	cin>>b;
	
	if(a>b)
	cout<<"Pemenangnya Adalah Virus = "<<a<<endl;
	
	else if(b<a)
	cout<<"Pemenangnya Adalah Virus = "<<a<<endl;
	
	cout<<"Pemenangnya adalah Virus = "<<a<<endl;
	cout<<"Selamat Yahhhh Untuk Virus "<<a;
	
}
